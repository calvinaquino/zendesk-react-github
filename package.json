{
	"name": "zendesk_github_react_app",
	"version": "0.1.0",
	"description": "Starting point for fetching github issues onto zendesk",
	"scripts": {
		"clean": "rimraf dist/tmp",
		"build": "webpack --config webpack.prod.js -p",
		"build-dev": "webpack --config webpack.dev.js",
		"package": "npm-run-all clean && zat package --path ./dist",
		"build:package": "npm-run-all build package",
		"watch": "webpack --config webpack.dev.js --watch",
		"serve": "zat server --path dist -c settings.json",
		"test": "jest"
	},
	"author": "Cloudhuset.dk",
	"license": "MIT",
	"devDependencies": {
		"@babel/core": "^7.2.2",
		"@babel/plugin-proposal-class-properties": "^7.4.4",
		"@babel/plugin-transform-runtime": "^7.2.0",
		"@babel/preset-env": "^7.2.3",
		"@babel/preset-react": "^7.0.0",
		"babel-eslint": "^10.0.1",
		"babel-jest": "^24.8.0",
		"babel-loader": "^8.0.5",
		"babel-preset-es2016": "^6.24.1",
		"css-loader": "^2.1.1",
		"eslint": "^5.14.1",
		"eslint-import-resolver-webpack": "^0.11.0",
		"eslint-plugin-import": "^2.16.0",
		"eslint-plugin-react": "^7.12.4",
		"eslint-watch": "^4.0.2",
		"html-loader": "^0.5.5",
		"html-webpack-plugin": "^3.2.0",
		"identity-obj-proxy": "^3.0.0",
		"jest": "^24.8.0",
		"npm-run-all": "^4.1.5",
		"rimraf": "^2.6.3",
		"sass-loader": "^7.1.0",
		"style-loader": "^0.23.1",
		"svg-inline-loader": "^0.8.0",
		"uglifyjs-webpack-plugin": "^2.1.3",
		"webpack": "^4.28.1",
		"webpack-cli": "^3.2.1",
		"webpack-merge": "^4.2.1"
	},
	"dependencies": {
		"@babel/runtime": "^7.2.0",
		"babel-preset-es2015": "^6.24.1",
		"babel-preset-react": "^6.24.1",
		"node-sass": "^4.12.0",
		"promise-polyfill": "^8.1.0",
		"react": "^16.8.6",
		"react-dom": "^16.8.6",
		"react-redux": "^7.0.3",
		"redux": "^4.0.1",
		"redux-thunk": "^2.3.0"
	},
	"jest": {
		"moduleNameMapper": {
			"\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
			"\\.(css|less|scss)$": "identity-obj-proxy"
		}
	}
}
